  <class name="Cultivation" table="Cultivation">
    
    <id name="Id" type="Int64" unsaved-value="0">
      <column name="Id" />
      <generator class="native" />
    </id>

    <!--//Name und Hierarchie-->
    <property name="CultivationDateStart" type="Int32"/>
    <property name="CultivationDateEnd" type="Int32"/>
    <property name="GerminationPeriodDays" type="Int32"/>
    <property name="GerminationTemperature" type="Double"/>
    
    
  </class>
</hibernate-mapping>
