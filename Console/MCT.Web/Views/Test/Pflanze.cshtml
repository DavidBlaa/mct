@model  MCT.Web.Models.Test.PflanzenModel



<g id="@Model.Name" class="pflanze" transform="matrix(1,0,0,1,@Model.X,@Model.Y)">
    <rect id="r" width="100" height="100" stroke="red" stroke-width="3px" fill="#eeeeee" />
    <text x="50" y="50"  alignment-baseline="middle" text-anchor="middle" fill="green" font-size="12" font-weight="bolder">@Model.Name</text>
</g>