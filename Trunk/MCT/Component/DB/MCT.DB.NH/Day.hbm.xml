  <class name="Day" table="Days">
    
    <id name="Id" type="Int64" unsaved-value="0">
      <column name="Id" />
      <generator class="native" />
    </id>

    <!--//Name und Hierarchie-->
    <property name="DayInYear" type="Int32"/>
    <property name="WeekPerYear" type="Int32"/>
    
  </class>
</hibernate-mapping>
