<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MCT.DB.Entities" namespace="MCT.DB.Entities">
  <class name="Media" table="Medias">
    <id name="Id" type="Int64" unsaved-value="0">
      <column name="Id" />
      <generator class="native" />
    </id>
    
    <many-to-one name="Subject" column="Subject" class="MCT.DB.Entities.Subject, MCT.DB.Entities" />
    
    <property name="ImagePath" type="String"/>
    <property name="MIMEType" type="String"/>
  </class>
</hibernate-mapping>
